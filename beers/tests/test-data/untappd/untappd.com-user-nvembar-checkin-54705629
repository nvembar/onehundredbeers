<script type="text/javascript">
$(document).ready(function() {
  $(document).on("click", ".track-click", function() {
      var d = $(this).attr("data-track");
      if ($(this).attr("data-href")) {
        d = d + $(this).attr("data-href");
      }
      _gaq.push(['_trackEvent', 'Click', 'Click', d]);
    });
   });
</script>
